<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1" />
    <meta name="description" content="" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0, maximum-scale=1.0, user-scalable=no" />

    <title>@ViewBag.Title</title>

    <link rel="apple-touch-icon-precomposed" sizes="144x144" href="~/Content/assets/images/icons/apple-touch-icon-144-precomposed.png" />
    <link rel="apple-touch-icon-precomposed" sizes="114x114" href="~/Content/assets/images/icons/apple-touch-icon-114-precomposed.png" />
    <link rel="apple-touch-icon-precomposed" sizes="72x72" href="~/Content/assets/images/icons/apple-touch-icon-72-precomposed.png" />
    <link rel="apple-touch-icon-precomposed" href="~/Content/assets/images/icons/apple-touch-icon-57-precomposed.png" />
    <link rel="shortcut icon" href="~/Content/assets/images/icons/favicon.png" />
    <!--[if lt IE 9]>
    <script src="~/Content/assets/js/minified/core/raphael.min.js"></script>
    <![endif]-->
    @*@Styles.Render("~/Content/css")
        @Scripts.Render("~/bundles/modernizr")*@

    <!-- Fides Admin CSS Core -->
    <link rel="stylesheet" type="text/css" href="~/Content/assets/css/minified/aui-production.min.css" />
    <!-- Theme UI -->
    <link id="layout-theme" rel="stylesheet" type="text/css" href="~/Content/assets/themes/minified/fides/color-schemes/dark-blue.min.css" />
    <!-- Fides Admin Responsive -->
    <link rel="stylesheet" type="text/css" href="~/Content/assets/themes/minified/fides/common.min.css" />
    <link rel="stylesheet" type="text/css" href="~/Content/assets/themes/minified/fides/responsive.min.css" />
    <!-- Fides Admin JS -->
    <script type="text/javascript" src="~/Content/assets/js/minified/aui-production.min.js"></script>

    <script>
            jQuery(window).load(
                function(){ 
                    var wait_loading = window.setTimeout( function(){
                      $('#loading').slideUp('fast');
                      jQuery('body').css('overflow','auto');
                    },1000
                    ); 
                });
            function back_block() {
                window.history.forward(1);
            }
    </script>

    <meta http-equiv="Content-Type" content="text/html; charset=utf-8" />
</head>
<body style="overflow: hidden;" onload="back_block()">

    <div id="loading" class="ui-front loader ui-widget-overlay bg-white opacity-100">
        <img src="~/Content/assets/images/loader-dark.gif" alt="" />
</div>
 
     
                @RenderBody()    

</body>
</html>
