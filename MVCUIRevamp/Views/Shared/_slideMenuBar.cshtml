@model IEnumerable<RithV.FX.EntityDTO.Security.WebParts>  
<div id="sidebar-menu">
    <ul>

        @helper _ShowTree(IEnumerable<RithV.FX.EntityDTO.Security.WebParts> Menus)
        {
        <Span>
            @foreach (var webmenu in Menus)
            {
                <li>

                    @if (webmenu.fldisParent)
                    {
                        <a href="javascript:;">
                            <i class="@webmenu.fldIconName"></i>
                            <span>@webmenu.fldDisplayName</span>
                        </a>
                    }
                    else
                    {
                        <a href="@Url.Action(webmenu.fldAction, webmenu.fldController)">
                            <i class="glyph-icon icon-chevron-right"></i>
                            <span>@webmenu.fldDisplayName</span>
                        </a>
                    }


                    @if (webmenu.fldWebPart.Any())
                    {
                        <ul>
                            @_ShowTree(webmenu.fldWebPart)
                        </ul>
                    }
                </li>
            }
        </Span>
}

